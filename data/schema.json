{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "UserID": {
        "type": "number"
      },
      "SubscriptionID": {
        "type": "number"
      },
      "CaseInfo": {
        "type": "object",
        "properties": {
          "ID": {
            "type": "number"
          },
          "CaseNumber": {
            "type": "string"
          },
          "Caption": {
            "type": "string"
          },
          "DateFiled": {
            "type": "string"
          },
          "Status": {
            "type": "string"
          },
          "CaseType": {
            "type": "string"
          },
          "PriorJurisdiction": {
            "type": "null"
          },
          "BadResponseText": {
            "type": "string"
          }
        }
      },
      "CaseJurisdiction": {
        "type": "object",
        "properties": {
          "Name": {
            "type": [
              "string",
              "null"
            ]
          },
          "County": {
            "type": [
              "string",
              "null"
            ]
          },
          "PriorDecisionDate": {
            "type": [
              "string",
              "null"
            ]
          },
          "PriorCaseNumbers": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "Number": {
                  "type": "string"
                }
              },
              "required": [
                "Number"
              ]
            }
          }
        }
      },
      "DocketItems": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "ID": {
              "type": "number"
            },
            "Description": {
              "type": "string"
            },
            "Code": {
              "type": "string"
            },
            "Type": {
              "type": "string"
            },
            "DateFiled": {
              "type": [
                "string",
                "null"
              ]
            },
            "DocumentName": {
              "type": "string"
            },
            "FilingParties": {
              "type": "string"
            }
          },
          "required": [
            "ID",
            "Description",
            "Code",
            "Type",
            "DateFiled",
            "DocumentName",
            "FilingParties"
          ]
        }
      },
      "DecisionItems": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "Description": {
              "type": "string"
            },
            "ReleaseDate": {
              "type": "string"
            },
            "DisposesCase": {
              "type": "boolean"
            },
            "DocumentName": {
              "type": "string"
            }
          },
          "required": [
            "Description",
            "ReleaseDate",
            "DisposesCase",
            "DocumentName"
          ]
        }
      },
      "Parties": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "Name": {
              "type": "string"
            },
            "ProSe": {
              "type": "boolean"
            },
            "Type": {
              "type": "string"
            },
            "Attorneys": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "Name": {
                    "type": "string"
                  },
                  "ARNumber": {
                    "type": "string"
                  },
                  "CounselOfRecord": {
                    "type": "boolean"
                  }
                },
                "required": [
                  "Name",
                  "ARNumber",
                  "CounselOfRecord"
                ]
              }
            }
          },
          "required": [
            "Name",
            "ProSe",
            "Type",
            "Attorneys"
          ]
        }
      }
    },
    "required": [
      "UserID",
      "SubscriptionID",
      "CaseInfo",
      "CaseJurisdiction",
      "DocketItems",
      "DecisionItems",
      "Parties"
    ]
  }
}
